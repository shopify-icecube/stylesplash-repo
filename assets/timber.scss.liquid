/* goto top */
#toTop {  
  position: fixed;
  right: 20px;
  bottom: 80px;
  cursor: pointer;
  width: 40px;
  height: 40px;
  background: $mainColor;
  outline: none;
  z-index: 2;
  border-radius: 4px;
  line-height: 28px;
  text-align: center;
}
#toTop::after {
    pointer-events: none;
    position: absolute;
    left: -1px;
    right: -1px;
    top: -1px;
    bottom: -1px;
    z-index: 1;
    content: '';
    box-sizing: content-box;
    transform: scale(0);
    opacity: 0;
    transition: all 0.3s cubic-bezier(0.41, 0.12, 1, 1);
    background: #000;
    border-radius: 4px;
}
#toTop:hover::after {
    transform: scale(1);
    opacity: 1;
    transition: all 0.3s cubic-bezier(0.41, 0.12, 1, 1);
}
#toTop i {
    color: #fff;
    font-size: 25px;
    line-height: 35px;
    vertical-align: middle;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}
#toTop:hover i {
    color: #fff;
    position: relative;
    z-index: 5;
}
